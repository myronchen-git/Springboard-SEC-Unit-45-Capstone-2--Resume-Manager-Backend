{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/schemas/text-snippet-version.json",
  "title": "Text Snippet Version",
  "description": "Format of text snippet version.",
  "type": "string",
  "pattern": "^20\\d{2}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])T([01][0-9]|2[0-3])(:([0-5][0-9])){2}\\.[0-9]{3}Z$",
  "examples": ["2025-01-01T00:00:00.000Z", "2099-12-30T23:59:59.999Z"]
}
