{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/schemas/user-signin.json",
  "type": "object",
  "title": "Signin User",
  "description": "Required info when signing in.",
  "required": ["username", "password"],
  "additionalProperties": false,
  "properties": {
    "username": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30,
      "examples": ["abc"]
    },
    "password": {
      "type": "string",
      "minLength": 6,
      "maxLength": 20,
      "examples": ["123Ab!"]
    }
  },
  "examples": [
    {
      "username": "abc",
      "password": "123Ab!"
    }
  ]
}
