{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/schemas/document_x_section-new.json",
  "type": "object",
  "title": "New Document-Section Relationship",
  "description": "Validates URL parameters.",
  "required": ["docId", "sectionId"],
  "additionalProperties": false,
  "properties": {
    "docId": {
      "type": "string",
      "pattern": "^\\d+$",
      "examples": ["1"]
    },
    "sectionId": {
      "type": "string",
      "pattern": "^\\d+$",
      "examples": ["1"]
    }
  },
  "examples": [
    {
      "docId": "1",
      "sectionId": "1"
    }
  ]
}
