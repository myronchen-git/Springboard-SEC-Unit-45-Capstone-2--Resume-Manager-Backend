{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/schemas/document-new.json",

  "title": "New Document",
  "description": "Required info for creating a new document.",

  "type": "object",
  "properties": {
    "documentName": {
      "type": "string",
      "minLength": 1,
      "examples": ["New document"]
    },
    "isTemplate": {
      "type": "boolean"
    }
  },
  "examples": [
    {
      "documentName": "New document",
      "isTemplate": "false"
    }
  ],
  "additionalProperties": false,
  "required": ["documentName", "isTemplate"]
}
